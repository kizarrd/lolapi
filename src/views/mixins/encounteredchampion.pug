mixin encounteredchampion_format(encounteredChampion)
    tr 
        td 
            img(src=`${DDRAGON}img/champion/${(encounteredChampion.id=='9')? 'Fiddlesticks' : championNameById[encounteredChampion.id][0]}.png`)
        td.champion=championNameById[encounteredChampion.id][1]
        td=encounteredChampion.playedAgainst
        td=encounteredChampion.playedWith
        td #{(encounteredChampion.playedAgainst == 0)? 'x' : encounteredChampion.winAgainst}
        td #{(encounteredChampion.playedWith == 0)? 'x' : encounteredChampion.winWith}
        td #{(encounteredChampion.playedAgainst == 0)? 'x' : `${(encounteredChampion.winRateAgainst*100).toFixed()}%`}
        td #{(encounteredChampion.playedWith == 0)? 'x' : `${(encounteredChampion.winRateWith*100).toFixed()}%`}